index 87cdd762..1fcd7bd9 100644
--- a/tests/auto/core/core.pro
+++ b/tests/auto/core/core.pro
@@ -1,9 +1,12 @@
 TEMPLATE = subdirs
 
-SUBDIRS = ivigenerator \
-          servicemanagertest \
+QT_FOR_CONFIG += ivicore
